{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local",
        ".env"
      ],
      "defaultRegion": "europe-west1",
      "serviceAccount": "firebase-adminsdk-fbsvc@carrington-leak-detection.iam.gserviceaccount.com"
    }
  ],
  "storage": {
    "rules": "storage.rules"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "database": {
    "rules": "database.rules.json"
  },
  "emulators": {
    "auth": {
      "port": 9099
    },
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "storage": {
      "port": 9199
    },
    "apphosting": {
      "port": 5000
    },
    "ui": {
      "enabled": true
    }
  },
  "apphosting": {
    "backendId": "carrington-leak-backend",
    "rootDir": "client",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "functions"
    ]
  }
}
